@using Model;

@{
    List<Model.Member> MemberViewModel = Model;
}
<table class="table table-striped">
    <thead>
        <tr><th>Name</th><th>Address</th><th>Date of Birth</th><th>Age</th></tr>
    </thead>
    <tbody>
        @{
            foreach (Member MemberItem in MemberViewModel)
            {
                DateTime zeroTime = new DateTime(1, 1, 1);
                TimeSpan span = DateTime.Now - MemberItem.Dob;
                int years = (zeroTime + span).Year - 1;
                <tr><td>@MemberItem.Name</td><td>@MemberItem.Address</td><td>@MemberItem.Dob.ToShortDateString()</td><td>@years</td></tr>
            }
        }
    </tbody>
</table>
